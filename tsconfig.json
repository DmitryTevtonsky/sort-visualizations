{
  "compilerOptions": {
    "baseUrl": "src/",
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true
  },
  "typeRoots": [
    "node_modules/@types"
  ],
  "include": [
    "src",
    "libs",
    "proxy",
    "config-overrides.js"
  ],
  "exclude": [
    "reportWebVitals.js"
  ]
}
